LoadPackage("genss");
LoadPackage("chop");

gens := AtlasGenerators("Fi22",6).generators;
g := Group(gens);
SetInfoLevel(InfoGenSS,3);
S := StabilizerChain(g);
s := AtlasStraightLineProgram("Fi22",1).program;
hgens := ResultOfStraightLineProgram(s,gens);
cgens := List(hgens,CMat);
m := Module(cgens);
r := Chop(m);
soc := SocleSeries(m,r.db);
v := Unpack(soc.basis[1]);
ConvertToVectorRep(v);
S := StabilizerChain(g,rec( Cand := rec( points := [v], ops := [OnRight])));
